{
  "name": "amplenote-plugin-outliner",
  "version": "0.1.0",
  "description": "Describe me hard.",
  "main": "lib/plugin.js",
  "scripts": {
    "build": "node esbuild.js",
    "build:ts": "tsc && node esbuild.js",
    "build:manual": "node esbuild.js && node scripts/build-manual.mjs build/compiled.js out/compiled-manual.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:unit": "jest",
    "test:env:ci": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "format": "prettier --check src/**/*.ts",
    "format:fix": "prettier --write src/**/*.ts"
  },
  "author": "@mavalau",
  "license": "MIT",
  "testEnvironment": "jsdom",
  "type": "module",
  "dependencies": {
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.24",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "dotenv": "^17.2.3",
    "esbuild": "0.17.15",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  }
}
